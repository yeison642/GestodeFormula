<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="@color/colorBackPrincipal"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/txAlgebra"
        android:layout_width="413dp"
        android:layout_height="44dp"
        android:background="@color/colorTexSeleccionCate"
        android:gravity="center"

        android:textColor="#FFFFFF"
        android:textSize="18sp"
        tools:ignore="MissingConstraints" />

    <ListView
        android:id="@+id/lvAlgebra"
        android:layout_width="393dp"
        android:layout_height="383dp"
        android:layout_marginTop="28dp"
        android:background="@color/colorBackListView"
        android:textColor="#FFFFFF"
        app:layout_constraintTop_toBottomOf="@+id/txAlgebra"
        tools:layout_editor_absoluteX="9dp"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>